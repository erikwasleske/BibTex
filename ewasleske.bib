	doi = {10.1093/mnras/stab3486},
	eprint = {https://academic.oup.com/mnras/article-pdf/510/1/687/41836378/stab3486.pdf},
	issn = {0035-8711},
	journal = {Monthly Notices of the Royal Astronomical Society},
	month = {12},
	number = {1},
