	issn = {0035-8711},
	journal = {Monthly Notices of the Royal Astronomical Society},
	month = {04},
	number = {1},
	pages = {543-550},
	title = {{On the AGN nature of broad balmer emission in four low-redshift metal-poor galaxies}},
