	issn = {0035-8711},
	journal = {Monthly Notices of the Royal Astronomical Society},
	month = {06},
	number = {2},
	pages = {1315-1338},
	title = {{Demystifying the coronal-line region of active galactic nuclei: spatially resolved spectroscopy with the Hubble Space Telescope}},
	url = {https://doi.org/10.1111/j.1365-2966.2010.16533.x},
	volume = {405},
	year = {2010},
