	journal = {The Astrophysical Journal},
	month = {aug},
	number = {1},
	pages = {68},
	publisher = {The American Astronomical Society},
	title = {Active Dwarf Galaxy Database. I. Overlap between Active Galactic Nuclei Selected by Different Techniques},
