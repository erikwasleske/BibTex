	publisher = {American Astronomical Society},
	title = {{THEGALEXTIME} {DOMAIN} {SURVEY}. {II}. {WAVELENGTH}-{DEPENDENT} {VARIABILITY} {OF} {ACTIVE} {GALACTIC} {NUCLEI} {IN} {THE} {PAN}-{STARRS}1 {MEDIUM} {DEEP} {SURVEY}},
