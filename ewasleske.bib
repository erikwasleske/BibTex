	pages = {140},
	publisher = {The American Astronomical Society},
	title = {CLASS: Coronal Line Activity Spectroscopic Survey},
	url = {https://dx.doi.org/10.3847/1538-4357/ac8981},
	volume = {936},
	year = {2022},
	bdsk-url-1 = {https://dx.doi.org/10.3847/1538-4357/ac8981}}

