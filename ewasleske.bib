	eprint = {https://academic.oup.com/mnras/article-pdf/435/3/1904/9501900/stt1379.pdf},
	issn = {0035-8711},
	journal = {Monthly Notices of the Royal Astronomical Society},
	month = {09},
	number = {3},
	pages = {1904-1927},
	title = {{A tidal flare candidate in Abell 1795★†‡}},
	url = {https://doi.org/10.1093/mnras/stt1379},
