	journal = {The Astrophysical Journal},
	month = {feb},
	number = {2},
	pages = {185},
	publisher = {American Astronomical Society},
	title = {A Large Population of Luminous Active Galactic Nuclei Lacking X-Ray Detections: Evidence for Heavy Obscuration?},
	url = {https://doi.org/10.3847/1538-4357/abd185},
	volume = {908},
